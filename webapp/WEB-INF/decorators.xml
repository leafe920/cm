<?xml version="1.0" encoding="ISO-8859-1"?>

<decorators defaultdir="/WEB-INF/templete">
	<!-- Any urls that are excluded will never be decorated by Sitemesh -->
	<excludes>
		<pattern>/styles/*</pattern>
		<pattern>/content/upload/*</pattern>
		<pattern>/WEB-INF/view/*</pattern>
	</excludes>
	

	<decorator name="www" page="template.jsp">
		<pattern>/*.jsp</pattern>
	</decorator>

</decorators>